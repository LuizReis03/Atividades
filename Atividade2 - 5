programa {
	funcao inicio() {
		inteiro num1
		inteiro num2
		inteiro resultado
		
		escreva ("Entre com o primeiro nÃºmero: ")
		leia (num1)
		escreva ("Entre com o segundo nÃºmero: ")
		leia (num2)
		resultado = num1 + num2
		
		se (num1 > 20 )
		escreva (resultado +8)
		
		senao { 
		    escreva(resultado -5)
		}
		}
	}
